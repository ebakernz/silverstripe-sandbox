{
    "name": "plasticstudio/skeletor",
    "description": "The PlasticStudio base SilverStripe project installer",
    "require": {
        "silverstripe/recipe-plugin": "^1.3",
        "silverstripe/recipe-cms": "^4.7",
        "silverstripe/crontask": "^2.1",
        "silverstripe/dynamodb": "^4.0",
        "symbiote/silverstripe-gridfieldextensions": "^3.2",
        "undefinedoffset/sortablegridfield": "^2.0",
        "plasticstudio/iconfield": "^1",
        "plasticstudio/sitemap": "^2.2",
        "dnadesign/silverstripe-elemental": "^4",
        "unclecheese/display-logic": "^2.0",
        "jonom/focuspoint": "^3.1",
        "embed/embed": "^3.4",
        "jonom/silverstripe-betternavigator": "^5.2",
        "swordfox/silverstripe-shopify": "1.2.6.3"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    },
    "type": "silverstripe-project",
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/database.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ],
        "expose": [
            "app/client/dist",
            "app/client/assets"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "app/tests/behat/src/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "3.*",
        "silverstripe/behat-extension": "^4.2",
        "dnadesign/silverstripe-populate": "^2.0",
        "lekoala/silverstripe-debugbar": "2.0.4",
        "friendsofphp/php-cs-fixer": "^2.18"
    }
}
